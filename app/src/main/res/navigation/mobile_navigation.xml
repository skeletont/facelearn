<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_look">

    <fragment
        android:id="@+id/navigation_look"
        android:name="com.example.facelearn.ui.look.LookFragment"
        android:label="@string/title_look"
        tools:layout="@layout/fragment_look" />

    <fragment
        android:id="@+id/navigation_register"
        android:name="com.example.facelearn.ui.register.RegisterFragment"
        android:label="@string/title_register"
        tools:layout="@layout/fragment_register" />

    <fragment
        android:id="@+id/navigation_manage"
        android:name="com.example.facelearn.ui.manage.ManagementFragment"
        android:label="@string/title_manage"
        tools:layout="@layout/fragment_manage" />
</navigation>